{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "generators/generator-base.js"
        ]
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true,
        "default": {
