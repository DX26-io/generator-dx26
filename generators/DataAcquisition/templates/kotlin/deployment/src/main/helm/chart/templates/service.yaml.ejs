apiVersion: v1
kind: Service
metadata:
  name: {{ include "<%= codeAppName %>.fullname" . }}
  labels:
    {{- include "<%= codeAppName %>.labels" . | nindent 4 }}
spec:
  type: {{ .Values.service.type }}
  ports:
    - port: {{ .Values.service.port }}
      targetPort: http
      protocol: TCP
      name: http
  selector:
    {{- include "<%= codeAppName %>.selectorLabels" . | nindent 4 }}
